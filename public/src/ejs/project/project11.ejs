<%
type = "project"
page = "project"
popup = "false"
lnbTitle = "강영민"
%>
<% include ../_include/_header %>
<div class="container">
    <% include ../_include/_lnbSelectProject %>
    <div class="site-project type-green">
        <!--타이틀-->
        <div class="project-number art-only">
            <div class="project-number__title motion-up">
                <img class="project-number__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_tit.png" alt="05"/>
            </div>
        </div>
        <!--//타이틀-->

        <!--이름 정보-->
        <div class="project-head art-only">
            <div class="project-head__title">
                <img class="project-head__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn1_txt.png" alt=""/>
            </div>
            <div class="project-head__meta">
                <ul class="project-head__list">
                    <!--instagram-->
                    <li class="project-head__item">
                        <a href="https://www.instagram.com/1s1t_youngmin.k" class="project-head__link instagram" target="_blank">
                            <span class="sr-only">인스타바로가기</span>
                        </a>
                    </li>
                    <!--//instagram-->
                    <!--homepage-->
                    <li class="project-head__item">
                        <a href="https://1s-1t.com" class="project-head__link homepage" target="_blank">
                            <span class="sr-only">홈페이지바로가기</span>
                        </a>
                    </li>
                    <!--//homepage-->
                </ul>
            </div>
        </div>
        <!--//이름 정보-->

        <!--정보-->
        <div class="project-info">
            <div class="project-info__title">
                <img class="project-info__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn2_txt1.png" alt=""/>
            </div>
            <div class="project-info__contents motion-up">
                <img class="project-info__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn2_txt2.png" alt=""/>
            </div>
        </div>
        <!--//정보-->

        <!--슬라이드-->
        <div class="project-slide">
            <div class="swiper-container project-slide__container">
                <div class="project-slide__name">
                    <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn4_txt1.png" alt="" />
                </div>
                <div class="swiper-wrapper project-slide__wrapper">
                    <div class='swiper-slide project-slide__item'>
                        <div class="project-slide__wrap">
                            <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn4_img1.jpg" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-slide__item'>
                        <div class="project-slide__wrap">
                            <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn4_img2.jpg" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-slide__item'>
                        <div class="project-slide__wrap">
                            <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section1_ctn4_img3.jpg" alt=""/>
                        </div>
                    </div>
                </div>
            </div>
            <div class="swiper-pagination project-slide-pagination"></div>
        </div>
        <!--//슬라이드-->

        <!--view 타이틀-->
        <div class="project-view">
            <div class="project-view__title motion-up">
                <img class="project-view__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_tit.png" alt="now on view"/>
            </div>
        </div>
        <!--//view 타이틀-->

        <!--장소 타이틀(지점)-->
        <div class="project-point">
            <div class="project-point__title">
                <img class="project-point__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_txt1.png" alt="now on view"/>
            </div>
        </div>
        <!--//장소 타이틀(지점)-->

        <!--장소 슬라이드-->
        <div class="project-place project-place-slide">
            <div class="swiper-container project-place__container">
                <div class="swiper-wrapper project-place__wrapper">
                    <div class='swiper-slide project-place__item'>
                        <div class="project-place__wrap">
                            <img class="project-place__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_img1.png" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-place__item'>
                        <div class="project-place__wrap">
                            <img class="project-place__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_img2.png" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-place__item'>
                        <div class="project-place__wrap">
                            <img class="project-place__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_img3.png" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-place__item'>
                        <div class="project-place__wrap">
                            <img class="project-place__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_img4.png" alt=""/>
                        </div>
                    </div>
                </div>
            </div>
            <div class="swiper-pagination project-place-pagination"></div>
        </div>
        <!--//장소 슬라이드-->

        <!--장소 상세정보-->
        <div class="project-address">
            <div class="project-address__info">
                <img class="project-address__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_txt2.png" alt=""/>
            </div>
            <div class="project-address__wrap">
                <ul class="project-address__list">
                    <li class="project-address__item">
                        <input type="text" class="project-address__input" value="경기도고양시 일산동구 중앙로 1283 1층" id="ShareUrl">
                        <a href="" class="project-address__link js-url-copy">
                            <img class="project-address__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_ico1.png" alt="주소복사" />
                        </a>
                    </li>
                    <li class="project-address__item">
                        <a href="https://m.place.naver.com/place/12133972/location?subtab=location" target="_blank" class="project-address__link">
                            <img class="project-address__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_ico2.png" alt="길찾기" />
                        </a>
                    </li>
                    <li class="project-address__item">
                        <a href="tel:031-909-3273" class="project-address__link">
                            <img class="project-address__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/art/section2_ctn1_ico3.png" alt="전화걸기" />
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <!--//장소 상세정보-->

        <!--more-->
        <div class="project-button">
            <div class="project-button__wrap">
                <a href="project.html" class="project-button__link">
                    <img class="project-button__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/1/section2_ctn1_btn1.png" alt="" />
                </a>
            </div>
        </div>
        <!--//more-->
    </div>
</div>
<% include ../_include/_contentFooter %>
<% include ../_include/_cartFix %>
<% include ../_include/_footer %>